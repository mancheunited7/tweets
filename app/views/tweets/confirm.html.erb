<div class = "container">
    <div class = "col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
        <div class="tsubuyaki">以下の内容でつぶやく</div>
    <%= form_for(@tweets) do |f| %>
    <%= f.hidden_field :content %>
    
    <div class="naiyo">投稿内容:<%= @tweets.content %></div>
    <%= f.submit"登録",class:'btn btn-primary btn-block' %>
    <% end %>
    
    <%= form_for(@tweets,url: new_tweet_path,method:'get') do |f| %>
    <%= f.hidden_field :content %>
    <%= f.submit"戻る",name:'back',class:'btn btn-primary btn-block' %>
    <% end %>
</div>
</div>
    